  "errors": {
    "dimension_incompatible": "Impossible de {operation} des quantités avec dimensions {left} et {right}",
    "unit_conversion_incompatible": "Impossible de convertir de {from_unit} à {to_unit}: dimensions incompatibles",
    "constraint_violated": "Contrainte '{constraint}' violée",
    "dimensionless_required": "L'opération nécessite une quantité sans dimension, obtenu {dimension}",
    "unit_not_found": "Unité '{symbol}' introuvable"
  },

  "cli": {
    "checking_file": "Vérification du fichier: {filename}",
    "issues_found": "{count} problèmes trouvés",
    "no_issues": "Aucun problème trouvé",
    "lint_complete": "Vérification terminée"
  },

  "messages": {
    "conversion_warning": "Avertissement: conversion de {from_unit} à {to_unit}",
    "inference_suggestion": "Suggestion: la variable '{name}' pourrait avoir la dimension {dimension}",
    "locale_changed": "Langue changée en {locale}",
    "locale_not_found": "Langue '{locale}' introuvable, utilisation de {fallback}"
  }
}
